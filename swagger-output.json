  "securityDefinitions": {
    "apiKeyAuth": {
      "type": "apiKey",
      "in": "headers",
      "name": "authorization",
      "description": "請加上 API Token"
    }
  },
  "paths": {
    "/upload/": {
      "post": {
        "description": "",
        "parameters": [],
        "responses": {}
      }
