    "/posts/": {
      "get": {
        "tags": [
          "Posts"
        ],
        "summary": "取得所有貼文",
        "description": "如為登入狀態，回傳所有貼文",
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "description": "search=",
            "type": "string"
          },
          {
            "name": "timeSort",
            "in": "query",
            "description": "timeSort=asc (舊到新) or 不填寫 (新到舊)",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "取得所有貼文資料",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string",
                        "example": "628a0aea9acb6cb7d6b70d93"
                      },
                      "userId": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "_id": {
                              "type": "string",
                              "example": "6286d9983208cb01aafaa562"
                            },
                            "name": {
                              "type": "string",
                              "example": "rikku12@gmail.com"
                            }
                          }
                        }
                      },
                      "content": {
                        "type": "string",
                        "example": "我只是個測試唷!"
                      },
                      "likes": {
                        "type": "array",
                        "example": [],
                        "items": {}
                      },
                      "createdAt": {
                        "type": "string",
                        "example": "2022-05-22T10:05:30.803Z"
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        },
        "security": [
          {
            "apiKeyAuth": []
          }
        ]
      },
      "post": {
        "tags": [
          "Posts"
        ],
        "summary": "送出貼文",
        "description": "需登入狀態，才可以送出貼文",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "formdata 資料格式",
            "schema": {
              "type": "object",
              "properties": {
                "content": {
                  "type": "string",
                  "example": "內文必填"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "取得所有貼文資料",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string",
                        "example": "628a0aea9acb6cb7d6b70d93"
                      },
                      "userInfo": {
                        "type": "string",
                        "example": "6286d9983208cb01aafaa562"
                      },
                      "content": {
                        "type": "string",
                        "example": "我只是個測試唷!"
                      },
                      "likes": {
                        "type": "array",
                        "example": [],
                        "items": {}
                      },
                      "createdAt": {
                        "type": "string",
                        "example": "2022-05-22T10:05:30.803Z"
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "回傳錯誤訊息",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "你沒有輸入內容"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        },
        "security": [
          {
            "apiKeyAuth": []
          }
        ]
      }
    },
